{
  "name": "now-client",
  "version": "0.4.1",
  "description": "Node.js wrapper for the ðš« now API",
  "main": "./dist/now.js",
  "scripts": {
    "prepublish": "babel -d ./dist src && npm run gen-doc",
    "gen-doc": "jsdoc2md --src src/now.js -t README.hbs > README.md",
    "test": "xo && mocha test/index.js --compilers js:babel-register"
  },
  "files": [
    "dist"
  ],
  "repository": "zeit/now-client",
  "xo": {
    "esnext": true,
    "space": true,
    "semicolon": false,
    "env": [
      "node",
      "mocha"
    ],
    "rules": {
      "quote-props": 0
    }
  },
  "keywords": [
    "now",
    "api",
    "zeit",
    "deploy"
  ],
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "author": "Timo Lins <me@timo.sh> (https://timo.sh)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "jsdoc-to-markdown": "^1.3.6",
    "mocha": "^2.5.3",
    "xo": "^0.16.0"
  },
  "dependencies": {
    "axios": "^0.14.0"
  }
}
