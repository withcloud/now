{
  "name": "now-client",
  "version": "0.7.0",
  "description": "Node.js wrapper for the ùö´ now API",
  "main": "dist/index.js",
  "scripts": {
    "start": "gulp",
    "build": "gulp transpile",
    "prepublish": "npm run build",
    "test": "xo && ava --timeout=30s"
  },
  "files": [
    "dist"
  ],
  "repository": "zeit/now-client",
  "xo": {
    "esnext": true,
    "space": true,
    "semicolon": false,
    "env": [
      "node"
    ],
    "rules": {
      "quote-props": 0
    }
  },
  "keywords": [
    "now",
    "api",
    "zeit",
    "deploy"
  ],
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "author": "Timo Lins <me@timo.sh> (https://timo.sh)",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.16.0",
    "ava-spec": "^1.0.1",
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-cached": "^1.1.0",
    "xo": "^0.17.0"
  },
  "dependencies": {
    "request": "^2.76.0",
    "request-promise-native": "^1.0.3"
  }
}
